{
    "parser": "babel-eslint",
    "plugins": [
      "react",
      "react-native"
    ],
    "parserOptions": {
      "ecmaFeatures": {
          "jsx": true,
          "modules": true
      }
    },
    "extends" : [
      "eslint:recommended",
      "plugin:react/recommended",
      "airbnb-base"
    ],
    "rules" : {
      "class-methods-use-this" : 0
    }
  }